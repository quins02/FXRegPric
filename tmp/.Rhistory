setwd("C:/Users/Matthew/Documents/GitHub/FXRegPric/tmp")
CF <- read.table("CF.dat", header = FALSE, sep="\t")
BF <- read.table("BucketFit12.dat", header = FALSE, sep="\t")
S <- read.table("strike.dat", header = FALSE, sep="\t")
BF<-BF[1:which(BF==max(BF[,1])),]
CF<-CF[which(CF==(BF[,1][1])):which(CF==max(BF[,1])),]
lm.out = lm(CF[,2] ~ BF[,2])
par(mfrow=c(2,3))
plot(CF[,2],BF[,2],type = "p",pch=3,main="Comparision of Closed Form \n and Regression Pricing Values", xlab="Closed Form Value", ylab="Regression Pricing Value")
abline(0,1)
abline(lm.out,col="red")
plot(CF[,1],(CF[,2]-BF[,2]),type = "p",pch=3, main="Residual Vs Asset Price", xlab="Asset price", ylab="Residual")
abline(v=S)
plot(lm.out)
cor.test(CF[,2],BF[,2])
summary(lm.out)
